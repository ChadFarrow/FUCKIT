      "namedRegex": "^/feed\\-validator(?:/)?$"
    },
    {
      "page": "/offline",
      "regex": "^/offline(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/offline(?:/)?$"
    },
    {
      "page": "/playlist",
      "regex": "^/playlist(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/playlist(?:/)?$"
    },
    {
      "page": "/test-errors",
      "regex": "^/test\\-errors(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/test\\-errors(?:/)?$"
    },
    {
      "page": "/test-jdog",
      "regex": "^/test\\-jdog(?:/)?$",
