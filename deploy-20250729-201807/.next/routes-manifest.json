{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ],
      "regex": "^(?:/(.*))(?:/)?$"
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ],
      "regex": "^/api(?:/(.*))(?:/)?$"
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "regex": "^/_next/static(?:/(.*))(?:/)?$"
    }
  ],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/album/[id]",
      "regex": "^/album/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/album/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/admin/feeds/[id]",
      "regex": "^/api/admin/feeds/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/admin/feeds/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/admin/feeds/[id]/refresh",
      "regex": "^/api/admin/feeds/([^/]+?)/refresh(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/admin/feeds/(?<nxtPid>[^/]+?)/refresh(?:/)?$"
    },
    {
      "page": "/api/cache/artwork/[id]",
      "regex": "^/api/cache/artwork/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/cache/artwork/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/cache/audio/[id]",
      "regex": "^/api/cache/audio/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/cache/audio/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/optimized-images/[filename]",
      "regex": "^/api/optimized\\-images/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPfilename": "nxtPfilename"
      },
      "namedRegex": "^/api/optimized\\-images/(?<nxtPfilename>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/publishers/[id]",
      "regex": "^/api/publishers/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/publishers/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/test-dynamic/[id]",
      "regex": "^/api/test\\-dynamic/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/test\\-dynamic/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/publisher/[id]",
      "regex": "^/publisher/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/publisher/(?<nxtPid>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/about",
