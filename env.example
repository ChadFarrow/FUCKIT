# Bunny.net CDN Configuration
BUNNY_CDN_HOSTNAME=your-zone.b-cdn.net
BUNNY_CDN_ZONE=your-zone-name
BUNNY_CDN_API_KEY=your-api-key-here

# CDN URLs
NEXT_PUBLIC_CDN_URL=https://your-zone.b-cdn.net

# Site Configuration  
NEXT_PUBLIC_SITE_URL=https://your-domain.com
NEXT_PUBLIC_API_URL=https://your-domain.com/api

# Environment
NODE_ENV=development

# Optional: Custom Domain for Images
NEXT_PUBLIC_IMAGE_DOMAIN=your-domain.com 